{
  "compilerOptions": {
    "jsx": "react",
    "target": "ESNext", // Change this to ESNext
    "lib": [
      "dom",
      "es2019",
      "ESNext" // Add this
    ],
    "module": "ESNext", // Change to ESNext
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "bundler", // Change to bundler
    "allowImportingTsExtensions": true,
    "types": ["node"]
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
